{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha All",
      "program": "${workspaceFolder}/packages/http-node/node_modules/mocha/bin/_mocha",
      "cwd": "${workspaceFolder}/packages/http-node",
      "args": [
        "--opts",
        "${workspaceFolder}/packages/http-node/test/mocha.opts"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "smartStep": true,
      "sourceMaps": true
    }
  ]
}
