{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "unit:js",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
          "run", "test-unit:js", "--"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ],
      "autoAttachChildProcesses": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "unit:react-native",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
          "run", "test-unit:react-native", "--"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ],
      "autoAttachChildProcesses": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "integration:nativescript",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run", "test-integration:nativescript", "--", "--"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ],
      "autoAttachChildProcesses": true,
      "timeout": 20000
    },
    {
      "type": "node",
      "request": "launch",
      "name": "integration:node",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run", "test-integration:node", "--", "--"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ],
      "autoAttachChildProcesses": true,
      "timeout": 20000
    },
  ]
}
