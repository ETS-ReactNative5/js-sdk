{
  "private": true,
  "name": "kinvey-nativescript-sdk",
  "version": "3.9.2",
  "description": "Kinvey NativeScript SDK for developing NativeScript applications.",
  "homepage": "http://www.kinvey.com",
  "license": "Apache-2.0",
  "author": "Kinvey, Inc.",
  "main": "bundle.js",
  "typings": "kinvey.d.ts",
  "scripts": {
    "postinstall": "node postinstall.js",
    "preuninstall": "node preuninstall.js"
  },
  "dependencies": {
    "es6-promise": "4.1.0",
    "events": "1.1.1",
    "nativescript-hook": "0.2.1",
    "nativescript-sqlite": "1.1.7",
    "replace-in-file": "3.0.0-beta.2"
  },
  "keywords": [
    "Kinvey",
    "JavaScript",
    "NativeScript"
  ],
  "nativescript": {
    "hooks": [
      {
        "type": "before-prepare",
        "script": "hooks/before-prepare.js",
        "inject": true
      }
    ],
    "platforms": {
      "ios": "3.0.0",
      "android": "3.0.0"
    }
  }
}
